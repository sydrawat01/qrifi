{
  "name": "@sydrawat/react-boilerplate",
  "version": "1.0.0",
  "description": "A simple QR code generator for your wifi settings",
  "main": "./src/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sydrawat/qrifi.git"
  },
  "homepage": "https://github.com/sydrawat/qrifi#readme",
  "author": "Siddharth Rawat",
  "license": "MIT",
  "keywords": [
    "react",
    "qr",
    "qr-code",
    "typescript",
    "react-router",
    "react-dom",
    "redux",
    "redux-toolkit",
    "webpack",
    "babel",
    "scss"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
    "start": "webpack-dashboard -- webpack serve --config webpack.config.js --env env=dev",
    "docker:start": "webpack-dashboard -- webpack serve --config webpack.config.js --env env=dev",
    "prebuild": "rimraf build",
    "prebuild:info": "rimraf build",
    "build": "webpack --config webpack.config.js --env env=prod",
    "build:info": "webpack --config webpack.config.js --env env=prod info=true",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "4",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "prettier": "^2.3.0",
    "react-refresh": "^0.10.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.1.1",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dashboard": "^3.3.3",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bugs": {
    "url": "https://github.com/sydrawat/qrifi/issues"
  }
}
